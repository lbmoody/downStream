{{> navbar/navbar-block}}
<div class="row loaderRow">
    <div class="col-md-4">
        {{#if streams}}
        <div class="row">
            {{> streams/createStream-block}}
        </div>
        {{/if}}
        <div data-spy="scroll" data-target="#list-example" data-offset="0" class="scrollspy">
            <ul>
                {{#if streams}}
                {{> streams/streams-block}}
                {{/if}}
                {{#if rafts}}
                {{> rafts/rafts-block}}
                {{/if}}
            </ul>
        </div>
    </div>
    <div class="col-md-7">
        <div class="contentBox">
            <div data-spy="scroll" data-target="#list-example" data-offset="0" class="scrollspy">
                {{#if streams}}
                {{> streams/streamsMessage-block}}
                {{/if}}
                {{#if rafts}}
                {{> rafts/dropzone-block}}
                {{/if}}
            </div>
        </div>
    </div>
</div>
<script src="../js/dropZone.js"></script>
<script src="../js/rafts.js"></script>

